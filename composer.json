{
    "name": "postedin/oauth2-server-laravel",
    "description": "An OAuth 2.0 bridge for Laravel and Lumen",
    "keywords": ["oauth2", "oauth", "server", "api", "laravel", "lumen"],
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Degasperi",
            "email": "packages@lucadegasperi.com"
        },
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        },
        {
            "name": "Robert Clancy",
            "email": "robbo@postedin.com"
        },
        {
            "name": "Steven Hawley",
            "email": "shawley@fulfillment.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravel/lumen-framework": "^8.3.4",
        "league/oauth2-server": ">=8.4.3"
    },
    "require-dev": {
        "orchestra/testbench": ">=6.47.1",
        "phpunit/phpunit": ">=9.6.23",
        "phpspec/phpspec": ">=7.6",
        "mockery/mockery": ">=1.6.12"
    },
    "autoload": {
        "psr-4": {
            "LucaDegasperi\\OAuth2Server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LucaDegasperi\\OAuth2Server\\Tests\\": "tests"
        },
        "classmap": [
            "database"
        ],
        "files": [
            "tests/AbstractTestCase.php",
            "tests/integration/AbstractDBTestCase.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    }
}
